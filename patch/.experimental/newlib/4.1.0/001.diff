new file mode 100644
index 000000000..c1d969740
--- /dev/null
+++ newlib/libc/sys/bolthur/mman/map.c
@@ -0,0 +1,142 @@
+
+/**
+ * Written by Christian Freitag <cfreitag.cf@googlemail.com>
+ * Copyright 2020 (c) Christian Freitag
+ *
+ * Permission to use, copy, modify, and distribute this software
+ * is freely granted, provided that this notice is preserved.
+ */
+
+#include <string.h>
+#include <sys/mman.h>
+#include <errno.h>
+#include <sys/bolthur.h>
+
+/**
+ * @brief map pages of memory
+ *
+ * @param addr address to start mapping at
+ * @param len length to map
+ * @param prot mapping properties
+ * @param flags mapping flags
+ * @param filedes file descriptor
+ * @param off offset for file descriptor
+ * @return void* mapped address or -1 on error
+ *
+ * @todo Check handling of MAP_SHARED
+ * @todo Check handling of MAP_FIXED
+ * @todo Add support for file descriptor
+ */
+void* mmap(
+  void* addr,
+  size_t len,
+  int prot,
+  int flags,
+  int filedes,
+  off_t off
+) {
+  if (
+    // handle invalid length
+    0 == len
+    // handle missing private or shared
+    || (
+      ! ( flags & MAP_SHARED )
+      && ! ( flags & MAP_PRIVATE )
+      && ! ( flags & MAP_ANONYMOUS )
+    )
+  ) {
+    errno = EINVAL;
+    return MAP_FAILED;
+  }
+
+  // system call protection parameter
+  int sys_prot = MEMORY_ACQUIRE_PROTECTION_NONE;
+  if ( prot & PROT_READ ) {
+    sys_prot |= MEMORY_ACQUIRE_PROTECTION_READ;
+  }
+  if ( prot & PROT_WRITE ) {
+    sys_prot |= MEMORY_ACQUIRE_PROTECTION_WRITE;
+  }
+  if ( prot & PROT_EXEC ) {
+    sys_prot |= MEMORY_ACQUIRE_PROTECTION_EXECUTABLE;
+  }
+  // system call flag parameter
+  int sys_flag = 0;
+  if ( flags & MAP_SHARED ) {
+    sys_flag |= MEMORY_ACQUIRE_MAP_SHARED;
+  }
+  if ( flags & MAP_PRIVATE ) {
+    sys_flag |= MEMORY_ACQUIRE_MAP_PRIVATE;
+  }
+  if ( flags & MAP_FIXED ) {
+    sys_flag |= MEMORY_ACQUIRE_MAP_FIXED;
+  }
+  if ( flags & MAP_ANONYMOUS ) {
+    sys_flag |= MEMORY_ACQUIRE_MAP_ANONYMOUS;
+  }
+
+  // handle file descriptor
+  const char* name = NULL;
+  if ( -1 != filedes ) {
+    // FIXME: CONTACT VFS VIA MESSAGE AND GET NAME BY FILE DESCRIPTOR
+    // FIXME: CONSIDER POSSIBLE OFFSET
+    errno = EINVAL;
+    return MAP_FAILED;
+  }
+
+  // allocate memory
+  void* ret_addr = _memory_acquire( addr, len, sys_prot, sys_flag, name );
+  // handle system call error
+  if ( ( void* )-1 == ret_addr ) {
+    errno = ENOMEM;
+    return MAP_FAILED;
+  }
+
+  // clear acquired memory
+  memset( ret_addr, 0, len );
+  // return allocated memory
+  return ret_addr;
+}
+
+/**
+ * @brief unmap pages of memory
+ *
+ * @param addr address to unmap
+ * @param len length
+ * @return int result
+ */
+int munmap( void* addr, size_t len ) {
+  // handle invalid length
+  if ( 0 == len ) {
+    errno = EINVAL;
+    return -1;
+  }
+  // request release and return response
+  return _memory_release( addr, len );
+}
+
+/**
+ * @brief set protection of memory mapping
+ *
+ * @param addr
+ * @param len
+ * @param prot
+ * @return int
+ */
+int mprotect( void* addr, size_t len, int prot ) {
+  errno = ENOSYS;
+  return -1;
+}
+
+/**
+ * @brief synchronize memory with physical storage
+ *
+ * @param addr
+ * @param len
+ * @param flags
+ * @return int
+ */
+int msync( void* addr, size_t len, int flags ) {
+  errno = ENOSYS;
+  return -1;
+}
