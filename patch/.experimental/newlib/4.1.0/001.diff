@@ -0,0 +1,36 @@
+
+/**
+ * Bolthur system call numbers for the ARM processor.
+ * Written by Christian Freitag <cfreitag.cf@googlemail.com>
+ * Copyright 2020 (c) Christian Freitag
+ *
+ * Permission to use, copy, modify, and distribute this software
+ * is freely granted, provided that this notice is preserved.
+ */
+
+#include <sys/stat.h>
+#include <sys/types.h>
+#include <sys/fcntl.h>
+#include <sys/times.h>
+#include <sys/errno.h>
+#include <sys/time.h>
+#include <stdio.h>
+#include <stdint.h>
+#include <errno.h>
+
+#undef errno
+extern int errno;
+
+/**
+ * @brief Set errno if set from system call
+ *
+ * @param n
+ * @return int
+ */
+int _set_errno( int n ) {
+  if ( n < 0 ) {
+    errno = -n;
+    return -1;
+  }
+  return n;
+}
