@@ -0,0 +1,68 @@
+
+/**
+ * Bolthur system call numbers for the ARM processor.
+ * Written by Christian Freitag <cfreitag.cf@googlemail.com>
+ * Copyright 2020 (c) Christian Freitag
+ *
+ * Permission to use, copy, modify, and distribute this software
+ * is freely granted, provided that this notice is preserved.
+ */
+
+#include "bolthur-system-call.h"
+
+#if __thumb__
+# define FUNC(name) .type name, %function; .thumb_func; name:
+# define SET .thumb_set
+#else
+# define FUNC(name) .type name, %function; name:
+# define SET .set
+#endif
+
+#define GLOBAL(name) .global name; FUNC(name)
+#define SIZE(name) .size name, .-name
+
+#define SYSTEM_CALL(name) \
+	GLOBAL(_ ## name); \
+	svc #SYSTEM_CALL_ ## name; \
+	b _set_errno; \
+	SIZE(_ ## name)
+
+#define SYSTEM_CALL6(name) \
+	GLOBAL(_ ## name); \
+	push { r4 - r5 }; \
+	ldr r4, [ sp, #8 ]; \
+	ldr r5, [ sp, #12 ]; \
+	svc #SYSTEM_CALL_ ## name; \
+	pop { r4 - r5 }; \
+	b _set_errno; \
+	SIZE(_ ## name)
+
+// system calls
+SYSTEM_CALL( process_create )
+SYSTEM_CALL( process_exit )
+SYSTEM_CALL( process_id )
+
+// FIXME: ADD AGAIN, WHEN WORKING ON USER THREADS
+//SYSTEM_CALL( thread_create )
+//SYSTEM_CALL( thread_exit )
+//SYSTEM_CALL( thread_id )
+
+SYSTEM_CALL6( memory_acquire )
+SYSTEM_CALL( memory_release )
+
+//SYSTEM_CALL( message_acquire )
+//SYSTEM_CALL( message_release )
+//SYSTEM_CALL( message_send )
+//SYSTEM_CALL( message_receive )
+
+//SYSTEM_CALL( io_acquire )
+//SYSTEM_CALL( io_release )
+//SYSTEM_CALL( io_check )
+//SYSTEM_CALL( io_raise )
+
+//SYSTEM_CALL( interrupt_acquire )
+//SYSTEM_CALL( interrupt_release )
+
+// FIXME: remove finally, when system is usable
+SYSTEM_CALL( dummy_putc )
+SYSTEM_CALL( dummy_puts )
